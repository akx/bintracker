;; A color scheme based on Gruvbox Light by Pavel Pertsev
;; https://github.com/morhetz/gruvbox

(bt-color-scheme background: "#fbf1c7" ; bg
		 background-inactive: "#fbf1c7" ; bg
		 row-highlight-major: "#d5c4a1" ; bg2
		 row-highlight-minor: "#ebdbb2" ; bg1
		 cursor: "#504945" ; fg2
		 text: "#7c6f64" ; gray7
		 text-inactive: "#928374" ; gray8
		 text-1: "#d65d0e" ; orange
		 text-2: "#cc241d" ; red
		 text-3: "#d79921" ; yellow
		 text-4: "#b16286" ; purple
		 text-5: "#98971a" ; green
		 text-6: "#689d6a" ; aqua
		 text-7: "#458588" ; blue
		 )
