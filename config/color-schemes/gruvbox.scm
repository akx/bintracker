;; A color scheme based on Gruvbox Dark by Pavel Pertsev
;; https://github.com/morhetz/gruvbox

(bt-color-scheme background: "#282828" ; bg
		 background-inactive: "#282828" ; bg
		 row-highlight-major: "#504945" ; bg2
		 row-highlight-minor: "#3c3836" ; bg1
		 cursor: "#d5c4a1" ; fg2
		 text: "#a89984" ; gray7
		 text-inactive: "#928374" ; gray8
		 text-1: "#d65d0e" ; orange
		 text-2: "#cc241d" ; red
		 text-3: "#d79921" ; yellow
		 text-4: "#b16286" ; purple
		 text-5: "#98971a" ; green
		 text-6: "#689d6a" ; aqua
		 text-7: "#98971a" ; blue
		 )
