;; A color scheme based on Inkpot by Ciaran McCreesh
;; https://github.com/ciaranm/inkpot

(bt-color-scheme background: "#1f1f27"
		 background-inactive: "#1f1f27"
		 row-highlight-major: "#616161"
		 row-highlight-minor: "#404040"
		 cursor: "#2d2d44"
		 text: "#cfbfad"
		 text-inactive: "#cfbfad" ; TODO
		 text-1: "#cd8b00"
		 text-2: "#ffcd8b"
		 text-3: "#2b91af"
		 text-4: "#87cefa" ; magenta
		 text-5: "#87fac4" ; green
		 text-6: "#8b8bff" ; cyan
		 text-7: "#808bed" ; blue
		 )
