(bt-color-scheme background: "#141411" ; gray-darkest
		 background-inactive: "#141411" ; gray-darkest
		 row-highlight-major: "#383830" ; gray-darker
		 row-highlight-minor: "#272822" ; gray-dark
		 cursor: "#eee8d5" ; base02 TODO
		 text: "#e6e6e6" ; gray-light
		 text-inactive: "#595959" ; gray-lightest
		 text-1: "#f92672" ; magenta
		 text-2: "#fd5ff1" ; purple-light
		 text-3: "#e6db74" ; yellow
		 text-4: "#d33682" ; magenta
		 text-5: "#a6e22e" ; green
		 text-6: "#89bdff" ; blue-light
		 text-7: "#ae81ff" ; purple
		 )
