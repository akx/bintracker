;; A color scheme based on base16 by Chris Kempson
;; http://chriskempson.com/projects/base16/

(bt-color-theme background: "#ffffff" ; background
		background-inactive: "#ffffff" ; background
		row-highlight-major: "#d6d6d6" ; selection
		row-highlight-minor: "#efefef" ; current line
		cursor: "#eee8d5" ; base02 TODO
		text: "#4d4d4c" ; foreground
		text-inactive: "#8e908c" ; comment
		text-1: "#f5871f" ; orange
		text-2: "#c82829" ; red
		text-3: "#eab700" ; yellow
		text-4: "#4271ae" ; blue
		text-5: "#781c00" ; green
		text-6: "#3e999f" ; aqua
		text-7: "#8959a8" ; purple
		)
